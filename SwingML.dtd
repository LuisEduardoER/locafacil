	NAME					ID					#REQUIRED
	VALUE					CDATA				#REQUIRED
>

<!ELEMENT TABLEBROWSER (TBC|TBR|LISTENER|ERRORHANDLER)*>
<!ATTLIST TABLEBROWSER
	NAME									ID																					#REQUIRED
	AUTO-FIT								(True|False)																	#IMPLIED
	SELECTION-STYLE					(Cell-Single|Cell-Multiple|Row-Single|Row-Multiple)		#IMPLIED
	CELL-MANAGER-CLASS 			CDATA																			#IMPLIED
	ORIENTATION 						(North|South|Center|East|West)									#IMPLIED
	ROW-HEADER						(True|False)																	#IMPLIED
	TOOLTIP								CDATA																			#IMPLIED
	POST-STYLE							(Post-Selected|Post-All)												#IMPLIED
	POST-COLUMN						CDATA																			#IMPLIED
	MODE									(Single|Multiple)															#IMPLIED
	WIDTH									CDATA																			#IMPLIED
	HEIGHT									CDATA																			#IMPLIED
	POST-COLUMN-STATE 			(True|False)																	#IMPLIED
>

<!ELEMENT TBC (COMBOBOX|FORMATTEDTEXTFIELD)*>
<!ATTLIST TBC
	TEXT				CDATA						#REQUIRED
	ALIGN			(Left|Center|Right)	#IMPLIED
	TYPE				CDATA						#IMPLIED    
	ITEMS			CDATA						#IMPLIED 
	SORT			CDATA						#IMPLIED
	SORTABLE		CDATA						#IMPLIED
	WIDTH			CDATA						#IMPLIED
	HIDDEN  		(True|False)				#IMPLIED
	POSITION   	CDATA						#IMPLIED
>

<!ELEMENT TBR (TBD)+>
<!ATTLIST TBR
	VALUE	CDATA	#IMPLIED 
>

<!ELEMENT TBD (ICON)*>
<!ATTLIST TBD
	NAME		CDATA							#IMPLIED
	VALUE		CDATA							#IMPLIED 
	TEXT		CDATA							#IMPLIED
	ALIGN		(Left|Center|Right)				#IMPLIED
	TOOLTIP		CDATA							#IMPLIED
    EDITABLE	(True|False)					#IMPLIED  
    FONT		CDATA							#IMPLIED 
    FONT-SIZE	CDATA							#IMPLIED 
    FONT-STYLE	(BOLD|ITALIC|PLAIN|BOLDITALIC)	#IMPLIED 
    ICON		CDATA							#IMPLIED 
    START-EDIT	(True|False)					#IMPLIED 
    BACKGROUND  (Black|Blue|Cyan|Gray|Green|Magenta|Orange|Pink|Red|White|Yellow)	#IMPLIED       		
    FOREGROUND  (Black|Blue|Cyan|Gray|Green|Magenta|Orange|Pink|Red|White|Yellow)	#IMPLIED     			
>

<!ELEMENT TREE_TABLE_COLUMN EMPTY>
<!ATTLIST TREE_TABLE_COLUMN
	TEXT	CDATA	#IMPLIED
>

<!ELEMENT TREE_TABLE_TREE (TREE_TABLE_DATA|TREE_TABLE_NODE|TREE_TABLE_TREE)*>
<!ATTLIST TREE_TABLE_TREE
	NAME	ID	#REQUIRED
>

<!ELEMENT TREE_TABLE_DATA EMPTY>
<!ATTLIST TREE_TABLE_DATA
	TEXT		CDATA							#IMPLIED
	FONT		CDATA							#IMPLIED 
    FONT-SIZE	CDATA							#IMPLIED 
    FONT-STYLE	(BOLD|ITALIC|PLAIN|BOLDITALIC)	#IMPLIED 
    ICON		CDATA							#IMPLIED
    BACKGROUND  (Black|Blue|Cyan|Gray|Green|Magenta|Orange|Pink|Red|White|Yellow)	#IMPLIED       		
    FOREGROUND  (Black|Blue|Cyan|Gray|Green|Magenta|Orange|Pink|Red|White|Yellow)	#IMPLIED 
>

<!ELEMENT TREE_TABLE_NODE (TREE_TABLE_DATA)*>

<!ELEMENT TREE_TABLE_BROWSER (TREE_TABLE_BROWSER_COLUMN|TREE_TABLE_BROWSER_TREE)*>
<!ATTLIST TREE_TABLE_BROWSER
	NAME		ID								#REQUIRED
	TEXT		CDATA							#IMPLIED
	ORIENTATION (North|South|Center|East|West)	#IMPLIED
	POST-STYLE	(Post-Selected|Post-All)		#IMPLIED
	WIDTH					CDATA				#IMPLIED
	HEIGHT					CDATA				#IMPLIED
>

<!ELEMENT TREE_TABLE_BROWSER_COLUMN (COMBOBOX|FORMATTEDTEXTFIELD)*>
<!ATTLIST TREE_TABLE_BROWSER_COLUMN
	TEXT		CDATA	#IMPLIED
	TYPE		CDATA	#IMPLIED
	SORT		CDATA	#IMPLIED
	SORTABLE	CDATA	#IMPLIED
	WIDTH		CDATA	#IMPLIED
	POSITION	CDATA	#IMPLIED
>

<!ELEMENT TREE_TABLE_BROWSER_TREE (TREE_TABLE_BROWSER_DATA|TREE_TABLE_BROWSER_NODE|TREE_TABLE_BROWSER_TREE)*>
<!ATTLIST TREE_TABLE_BROWSER_TREE
	NAME	CDATA	#REQUIRED
	POST-STYLE	(Post-Selected|Post-All)	#IMPLIED
	OPEN-ICON	CDATA	#IMPLIED
	CLOSED-ICON	CDATA	#IMPLIED
	LEAF-ICON	CDATA	#IMPLIED
>

<!ELEMENT TREE_TABLE_BROWSER_DATA (ICON)*>
<!ATTLIST TREE_TABLE_BROWSER_DATA
	NAME	CDATA	#IMPLIED
	TEXT	CDATA	#IMPLIED
    EDITABLE (TRUE|FALSE)   #IMPLIED
    TOOLTIP CDATA	#IMPLIED
    FONT		CDATA							#IMPLIED 
    FONT-SIZE	CDATA							#IMPLIED 
    FONT-STYLE	(BOLD|ITALIC|PLAIN|BOLDITALIC)	#IMPLIED 
    ICON		CDATA							#IMPLIED
    BACKGROUND  (Black|Blue|Cyan|Gray|Green|Magenta|Orange|Pink|Red|White|Yellow)	#IMPLIED       		
    FOREGROUND  (Black|Blue|Cyan|Gray|Green|Magenta|Orange|Pink|Red|White|Yellow)	#IMPLIED   
>

<!ELEMENT TREE_TABLE_BROWSER_NODE (TREE_TABLE_BROWSER_DATA)*>
<!ATTLIST TREE_TABLE_BROWSER_NODE
	NAME	CDATA	#IMPLIED
>

<!ELEMENT REMOTE_ACTION_RESULT (ERROR|NO-OP|UPDATE)*>

<!ELEMENT ERROR EMPTY>
<!ELEMENT NO-OP EMPTY>
<!ELEMENT UPDATE EMPTY>
<!ATTLIST UPDATE
	ROWID	CDATA	#REQUIRED
	COLID	CDATA	#IMPLIED    
	VALUE	CDATA	#IMPLIED 
>
<!ELEMENT PERFORM EMPTY>
<!ATTLIST PERFORM
	COMPONENT   CDATA	#REQUIRED
	METHOD	    CDATA	#REQUIRED
	TYPE 	    CDATA	#REQUIRED
	VALUE	    CDATA	#REQUIRED
>
<!ELEMENT ICON (LISTENER)*>
<!ATTLIST ICON
	LOCATION	CDATA	#IMPLIED
	TEXT		CDATA	#IMPLIED
	TOOLTIP		CDATA	#IMPLIED
>